<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ucd.urbanflow.mapper.LaneMapper">

    <select id="getAllLaneEdgeMappings" resultType="com.ucd.urbanflow.model.LaneEdgeInfo">
        SELECT
            lane_id AS laneId,
            edge_name AS edgeName,
            lane_shape AS laneShape,
            edge_id AS edgeId
        FROM
            lane
    </select>


</mapper>
