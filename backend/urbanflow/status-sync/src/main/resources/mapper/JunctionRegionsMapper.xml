<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ucd.urbanflow.mapper.JunctionRegionsMapper">

    <select id="findJunctionIdsByArea" resultType="java.lang.String">
        SELECT junction_id FROM junction_regions
        WHERE area = #{area}
    </select>

</mapper>