<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ucd.urbanflow.mapper.TrafficFlowMapper">

    <select id="selectByJunctionAndTimeRange" resultType="com.ucd.urbanflow.model.TrafficFlow">
        SELECT * FROM traffic_flow
        WHERE junction_id = #{junctionId}
            AND time_bucket BETWEEN #{start} AND #{end}
    </select>

</mapper>
